{{#each faq.results}}
<div class="expandable-block expandable-block--support" id="{{id}}" data-articleNumber="{{articleNumber}}">
	<h4 class="expandable-block-header expandable-block--support-header">
		<button class="expandable-block-trigger support-faq-trigger" aria-expanded="false">{{question}}</button>
		 <i class="support-faq-open"><svg width="16" height="16" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg"><title>Expand FAQ</title><path d="M9.23 6.666V1.27C9.23.714 8.78.26 8.223.263h-.504a1.01 1.01 0 0 0-1.01 1.008v5.544H1.164c-.556 0-1.01.452-1.01 1.01v.504c0 .557.452 1.01 1.01 1.01H6.56v5.394c0 .558.45 1.01 1.008 1.01h.504c.557 0 1.01-.453 1.01-1.01V9.188h5.544a1.01 1.01 0 0 0 1.01-1.01l-.002-.504c0-.557-.45-1.01-1.01-1.01l-5.394.002z" fill="#807370"/></svg></i>
		 <i class="support-faq-close"><svg width="16" height="3" viewBox="0 0 16 3" xmlns="http://www.w3.org/2000/svg"><title>icon-minimise</title><rect x="817" y="1165" width="16" height="3" rx="1.5" transform="translate(-817 -1165)" fill="#5E514D" fill-rule="evenodd"/></svg></i>
	</h4>

	<article class="expandable-block-content expandable-block-content--support">

		<div class="expandable-block-content-inner">{{{answer}}}</div>


		<div class="article-feedback">
	    <div class="af-questions">
		    <h4 class="af-question">Was this answer helpful?</h4>
		    <div class="af-controls">
			    <button class="af-positive btn-unstyled"><span class="sr-only">Was this answer helpful?</span>Yes</button>
			    <button class="af-negative btn-unstyled"><span class="sr-only">Was this answer helpful?</span>No</button>
		    </div>
		   </div>
		   <form class="af-form-negative">
		    <div class="form-group">
			    <label class="af-form-label" for="form-textarea-medium">If you have any other feedback about this answer (no account info, please), let us know below:</label>
			    <textarea name="feedbackText" placeholder="Tell us more" maxlength="200" class="form-control af-form-textarea"></textarea>
			    <span class="af-form-error">Please enter feedback.</span>
		    </div>
		    <button class="btn btn-dark af-form-submit" type="submit">Send feedback</button>
		    <button class="btn btn-unstyled af-form-cancel" type="submit">Cancel</button>
		   </form>
		   <div class="af-results">
		    <p class="af-loading-container">Sending feedback.</p>
		    <p class="af-error-text">An error has occurred. Please try again later.</p>
		    <p class="af-success-text">
		    	<svg class="icon-tick" width="50" height="19" viewBox="3 3 19 19" xmlns="http://www.w3.org/2000/svg"><path d="M12.2 3C7.12 3 3 7.123 3 12.202c0 5.08 4.12 9.198 9.2 9.198 5.076 0 9.2-4.117 9.2-9.198C21.4 7.122 17.275 3 12.2 3m4.84 6.812l-6.584 6.586a.982.982 0 0 1-.688.284l-.102-.004a.978.978 0 0 1-.713-.438L6.95 13.135a.97.97 0 0 1 1.633-1.053l1.35 2.092 5.734-5.733a.97.97 0 1 1 1.372 1.372" fill-rule="evenodd"/></svg>
		    	<span>Thanks for your feedback</span>
		    </p>
	    </div>

   		<a href="{{somewhere}}" class="link link-secondary link-icon-right af-more-help">
   			<svg class="link-icon-right-svg" width="4" height="7" class="tt-icon" viewBox="0 0 4 7" xmlns="http://www.w3.org/2000/svg"><path d="M3.646 2.674l-.013-.008L1.13.16a.547.547 0 0 0-.776 0L.16.355a.548.548 0 0 0 0 .775l2.13 2.128L.16 5.386a.55.55 0 0 0 0 .775l.194.194c.214.214.56.214.775 0L3.632 3.85c.004-.005.01-.006.013-.01l.194-.193a.546.546 0 0 0 .16-.39.545.545 0 0 0-.16-.39l-.194-.193z" fill-rule="evenodd"></path></svg>
   			<span class="link-icon-right-text">Still need help? Contact us</span>
   		</a>
    </div>
	</article>
</div>
{{/each}}
